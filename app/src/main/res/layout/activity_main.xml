<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/Our_team"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Our Team"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/view1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:elevation="12dp"
        app:cardCornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1">

        <ImageView
            android:id="@+id/i1"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/me" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/view2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:elevation="12dp"
        app:cardCornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1">

        <ImageView
            android:id="@+id/i2"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/unnamed" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/view3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:elevation="12dp"
        app:cardCornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/steve" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/t1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Min Han"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view1"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="50dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view1" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Steve Job"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view2"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="50dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view2" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Steven"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view3"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="50dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view3" />

    <TextView
        android:id="@+id/t11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Junior Android Developer"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view1"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="65dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/t11" />

    <TextView
        android:id="@+id/t12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CEO of Apple"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view2"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="65dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/t2" />

    <TextView
        android:id="@+id/t13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Co-Founder Of apple"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view3"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="65dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/t3" />


    <androidx.cardview.widget.CardView
        android:id="@+id/view11"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:elevation="12dp"
        app:cardCornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.33">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/d" />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/view22"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:elevation="12dp"
        app:cardCornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.33">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/dog" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/view33"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:elevation="12dp"
        app:cardCornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.33">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/study" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/tt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jakie"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view11"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="50dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view11" />

    <TextView
        android:id="@+id/tt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mae Lone"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view22"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="50dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view22" />

    <TextView
        android:id="@+id/tt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kyaw Gyii"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view33"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="50dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view33" />
    <TextView
        android:id="@+id/tt11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Junior Consultant"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view11"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="65dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tt1" />

    <TextView
        android:id="@+id/tt12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Marketing Supervisor"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view22"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="65dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tt2" />

    <TextView
        android:id="@+id/tt13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senior Web Designer"
        android:textSize="12sp"
        app:layout_constraintCircle="@id/view33"
        app:layout_constraintCircleAngle="180"
        app:layout_constraintCircleRadius="65dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tt3" />

    <TextView
        android:id="@+id/mission"
        android:layout_marginTop="15dp"
        android:textSize="25sp"
        app:layout_constraintTop_toBottomOf="@id/tt12"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="Mission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/para"
        android:textSize="18sp"
        android:text="@string/paragraph_one"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@id/mission"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="wrap_content"
        android:ems="18"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/paraend"
        android:textSize="18sp"
        android:text="@string/bb"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/para"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="wrap_content"
        android:ems="18"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>
